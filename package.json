{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=dev nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "NODE_ENV=dev nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/main",
    "start:cli": "MODE=CLI nest start -- ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "husky install",
    "prepare": "husky install",
    "commitlint": "commitlint --edit",
    "release": "release-it"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@contact/models": "^0.4.3",
    "@fastify/static": "^6.12.0",
    "@grpc/grpc-js": "^1.9.12",
    "@grpc/proto-loader": "^0.7.10",
    "@mui/x-data-grid-premium": "^6.18.2",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/microservices": "^10.2.10",
    "@nestjs/platform-fastify": "^10.2.10",
    "@nestjs/platform-socket.io": "^10.2.10",
    "@nestjs/platform-ws": "^10.2.10",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/websockets": "^10.2.10",
    "@sql-tools/nestjs-sequelize": "^10.0.0",
    "@sql-tools/sequelize": "^6.32.0",
    "@sql-tools/sequelize-typescript": "^2.1.5",
    "@tools/nestjs-smb2": "^1.1.0",
    "@tools/terminus-indicators": "^1.0.3",
    "axios": "^1.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "colors": "^1.4.0",
    "crypto-js": "^4.2.0",
    "exceljs": "^4.4.0",
    "git-semver-tags": "^5.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nest-commander": "^3.12.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "tedious": "^16.6.1",
    "umzug": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@release-it/conventional-changelog": "^7.0.2",
    "@sql-tools/association-literal": "^0.0.3",
    "@types/crypto-js": "^4.2.1",
    "@types/git-semver-tags": "^4.1.3",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4",
    "@types/node": "^20.10.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "release-it": "^16.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@4.0.2"
}
